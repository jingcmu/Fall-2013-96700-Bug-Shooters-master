<% provide(:title, 'adhoc_deliverable show') %>

<div class="container-fluid">

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @adhoc_deliverable.name%></dd>
</dl>

<dl class="dl-horizontal">
  <dt>Description:</dt>
  <dd><%= @adhoc_deliverable.description%></dd>
</dl>


<dl class="dl-horizontal">
  <dt>Complexity:</dt>
  <dd><%=@adhoc_deliverable.complexity %></dd>
</dl>

<dl class="dl-horizontal">
  <dt>Unit of Measure:</dt>
  <dd><%=@adhoc_deliverable.unit_of_measure %></dd>
</dl>

<dl class="dl-horizontal">
  <dt>Estimated Size:</dt>
  <dd><%=@adhoc_deliverable.estimated_size %></dd>
</dl>

<dl class="dl-horizontal">
  <dt>Production Rate:</dt>
  <dd><%=@adhoc_deliverable.estimated_production_rate %></dd>
</dl>

<dl class="dl-horizontal">
  <dt>Estimated Effort:</dt>
  <dd><%=@adhoc_deliverable.estimated_effort %></dd>
</dl>


<p>
  <button class="btn" type = "button">
    <%= link_to 'Edit', edit_deliverable_path(@adhoc_deliverable) %>
  </button>

  <button class="btn" type = "button">
    <%= link_to 'Delete', @adhoc_deliverable, method: :delete, data: { confirm: 'Are you sure?' } %>
  </button>

 </p>


</div>


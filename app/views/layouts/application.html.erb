<!DOCTYPE html>
<html>

<head>
  <title><%= yield(:title) %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>

<body style = "background-size: cover" style = "height: 100%">
    <%= render 'layouts/header' %>
    <div>
      <div class="container">
          <% flash.each do |key, value| %>
              <div class="alert alert-<%= key %>"><%= value %></div>
          <% end %>
          <%= yield %>
          <%= yield :scripts %>
      </div>


    </div>
    <div></div>
    <div></div>
    <div></div>
    <br />
    <br />
    <br />
    <br />
    <br />
</body>

<div class="container">
  <hr></hr>
  <%= render 'layouts/footer' %>
</div>
</html>

